{
    "version": 2,
    "functions": {
    "api/*": {
      "maxDuration": 30
    }
},
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE","OPTIONS"]
      }
    ]
  }
  